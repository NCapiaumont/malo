<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ihm.malo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CalendarActivity"
            android:label="@string/title_activity_calendar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LosingPasswordActivity" />
        <activity android:name=".CreateAccountActivity" />
        <activity android:name=".NoFamilyActivity" />
        <activity android:name=".CreateFamilyActivity" />
        <activity android:name=".FamilyActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".shoppingActivity"></activity>
    </application>

</manifest>
